<div class="container-fluid">
	<table class="table table-striped">
		<thead>
			<tr>
				<th></th>
				<th>ID</th>
				<th>Name</th>
				<th>Address</th>
				<th>Full Address</th>
				<th>Accessible</th>
				<th>Parking</th>
				<th>Isflat</th>
				<th>Elevator</th>
				<th>create</th>
			</tr>
		</thead>
		<tbody>
			<%@hash5.each_with_index do |h, index|%>
				<tr>
					<%output = h.to_a%>
					<td><%=index+1%></td>
					
					<td><%=output[0][1]%></td>
					<td><%=output[1][1]%></td>
					<td><%=output[2][1]%></td>
					<td><%=output[3][1]%></td>
						
					<td><%=output[7][1]%></td>
					<td><%=output[8][1]%></td>
					<td><%=output[9][1]%></td>
					<td><%=output[10][1]%></td>
					<td>
						<%=form_tag restaurants_create_path do %>
							<input type="hidden" name="id" value="<%=output[0][1]%>">
							<input type="hidden" name="name" value="<%=output[1][1]%>">
							<input type="hidden" name="address" value="<%=output[2][1]%>">
							
							<input type="hidden" name="fulladd" value="<%=output[3][1]%>">
							<input type="hidden" name="accessible" value="<%=output[7][1]%>">
							<input type="hidden" name="parking" value="<%=output[8][1]%>">
							<input type="hidden" name="isflat" value="<%=output[9][1]%>">
							<input type="hidden" name="elevator" value="<%=output[10][1]%>">
							
							<input type="submit" value="create" class="btn btn-warning">
						<% end %>
					</td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
